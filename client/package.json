{
	"name": "client",
	"version": "1.0.0",
	"main": "index.js",
	"license": "MIT",
	"scripts": {
		"story": "start-storybook -p 6006 -c .storybook",
		"prebuild": "rimraf dist",
		"build": "webpack --config webpack.config.prod.js",
		"dev-renderer": "webpack-dev-server --config webpack.config.dev.js",
		"dev-main": "nodemon",
		"dev-main:2": "nodemon --config nodemon.2.json",
		"dev-main:3": "nodemon --config nodemon.3.json",
		"dev": "concurrently -k -p \"[{name}]\" -n \"renderer,main\" -c \"cyan.bold,yellow.bold\" \"yarn dev-renderer\" \"yarn dev-main\"",
		"dev:2": "concurrently -k -p \"[{name}]\" -n \"renderer,main\" -c \"cyan.bold,yellow.bold\" \"yarn dev-renderer\" \"yarn dev-main:2\"",
		"dev:3": "concurrently -k -p \"[{name}]\" -n \"renderer,main\" -c \"cyan.bold,yellow.bold\" \"yarn dev-renderer\" \"yarn dev-main:3\""
	},
	"dependencies": {
		"classnames": "^2.2.6",
		"react": "^16.8.6",
		"react-dom": "^16.8.6",
		"socket.io-client": "^2.2.0"
	},
	"devDependencies": {
		"@storybook/addon-knobs": "^5.1.9",
		"@storybook/react": "^5.1.9",
		"@types/classnames": "^2.2.8",
		"@types/react": "^16.8.22",
		"@types/react-dom": "^16.8.4",
		"@types/socket.io-client": "^1.4.32",
		"@types/storybook__addon-knobs": "^5.0.1",
		"@types/storybook__react": "^4.0.2",
		"@types/webpack": "^4.32.1",
		"@types/webpack-env": "^1.13.9",
		"awesome-typescript-loader": "^5.2.1",
		"concurrently": "^4.1.1",
		"css-loader": "^3.0.0",
		"electron": "^6.0.0",
		"html-webpack-plugin": "^3.2.0",
		"node-sass": "^4.12.0",
		"nodemon": "^1.19.1",
		"rimraf": "^2.6.3",
		"sass-loader": "^7.1.0",
		"style-loader": "^0.23.1",
		"ts-node": "^8.3.0",
		"typescript": "^3.5.2",
		"url-loader": "^2.0.0",
		"webpack": "^4.35.0",
		"webpack-cli": "^3.3.5",
		"webpack-dev-server": "^3.7.2",
		"webpack-merge": "^4.2.1"
	}
}
